$("#log_result").val('');
$("#start-work").css('display', 'block');
$("#start-break").css('display', 'none');
<% l = Log.last %>
$("#feed").prepend('<div class="feed-item">\
						<p class="text-muted" style="font-size: 10pt; text-align: center"><%= l.created_at.strftime("%B %e, %Y - %l:%M%P") %></p>\
						<div class="clearfix"></div>\
						<div class="row">\
							<div class="col-sm-6">\
								<p class="task-feed text-danger"><%= l.task %></p>\
							</div>\
							<div class="col-sm-6">\
								<p class="result-feed text-info"><%= l.result %></p>\
							</div>\
						</div>\
					</div>');